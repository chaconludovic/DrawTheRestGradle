<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.eldoraludo.drawtherest"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="10"
        android:targetSdkVersion="15" />

    <uses-permission android:name="android.permission.RECEIVE_SMS" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.GET_TASKS" />

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".DrawTheRest"
            android:label="@string/title_activity_draw_the_rest_with_view" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SettingsActivity"
            android:label="@string/title_settings_activity" >
        </activity>
        <activity
            android:name=".DrawingActivityWitView"
            android:label="@string/title_activity_drawing_activity_with_view" >
        </activity>
    </application>

</manifest>